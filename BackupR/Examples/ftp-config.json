{
  "Name": "ftp",
  "Type": "Tekook.BackupR.Lib.Ftp.FtpProvider",
  "Provider": {
    "Host": "ftp.dev.local",
    "Path": "/",
    "Username": "docker-dev",
    "Password": "docker-dev"
  },
  "Backup": {
    "Folders": [
      {
        "Name": "Home-Backup",
        "UploadName": "home_$date.tar.gz",
        "UploadPath": "home",
        "Path": "C:\\tmp\\b\\home",
        "Compress": true,
        "Excludes": [
          "C:\\tmp\\b\\asd"
        ]
      }
    ],
    "Commands": [
      {
        "Name": "Command-Backup",
        "UploadName": "etc_$date",
        "UploadPath": "etc",
        "Command": "powershell",
        "Arguments": "C:\\tmp\\b\\script.ps1"
      }
    ],
    "MysqlBackups": [
      {
        "Name": "Mysql-Backup",
        "UploadPath": "mysql",
        "MysqlDumpPath": "mysqldump"
      }
    ]
  },
  "Containers": [
    {
      "Path": "etc",
      "MaxSize": "1GB",
      "MaxFiles": 10
    },
    {
      "Path": "etc2",
      "MaxSize": "1GB",
      "MaxFiles": null
    }
  ]
}