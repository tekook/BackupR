{
  "Name": "ftp",
  "Type": "Tekook.BackupR.Lib.Ftp.FtpProvider",
  "Provider": {
    "Host": "ftp.dev.local",
    "Path": "/",
    "Username": "docker-dev",
    "Password": "docker-dev"
  },
  "Backup": {
    "Folders": [
      {
        "Name": "Home-Backup",
        "Path": "C:\\tmp\\home",
        "Compress": true,
        "Excludes": []
      }
    ],
    "Commands": [
      {
        "Name": "Command-Backup",
        "Command": "powershell C:\\tmp\\script.ps1",
        "OutputFile": "C:\\tmp\\script_output.zip"
      }
    ],
    "MysqlBackups": [
      {
        "Name": "Mysql-Backup",
        "MysqlDumpPath": "mysqldump"
      }
    ]
  },
  "Containers": [
    {
      "Path": "etc",
      "MaxSize": "1GB",
      "MaxFiles": 10
    },
    {
      "Path": "etc2",
      "MaxSize": "-1GB",
      "MaxFiles": null
    }
  ]
}