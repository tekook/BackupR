{
  "Name": "ftp",
  "Type": "Tekook.BackupR.Lib.Ftp.FtpProvider",
  "Provider": {
    "Host": "ftp.dev.local",
    "Path": "/",
    "Username": "docker-dev",
    "Password": "docker-dev"
  },
  "Backups": [
    {
      "Name": "Home-Backup",
      "Type": "Tekook.BackupR.Lib.Backups.Folder",
      "Path": "C:\\tmp\\home",
      "Compress": true,
      "Excludes": []
    },
    {
      "Name": "Command-Backup",
      "Type": "Tekook.BackupR.Lib.Backups.Command",
      "Command": "powershell C:\\tmp\\script.ps1",
      "OutputFile":  "C:\\tmp\\script_output.zip"
    },
    {
      "Name": "Mysql-Backup",
      "Type": "Tekook.BackupR.Lib.Backups.Mysql",
      "MysqlDumpPath": "mysqldump",


    }
  ],
  "Containers": [
    {
      "Path": "etc",
      "MaxSize": "1GB",
      "MaxFiles": 10
    },
    {
      "Path": "etc2",
      "MaxSize": "-1GB",
      "MaxFiles": null
    }
  ]
}