{
  "Name": "ftp",
  "Type": "Tekook.BackupR.Lib.Providers.FsProvider",
  "StateFile": "state.json",
  "Provider": {
    "Path": "C:\\tmp\\data"
  },
  "Backup": {
    "Folders": [
      {
        "Disabled": false,
        "Name": "Home-Backup",
        "UploadName": "home_$date.tar.gz",
        "UploadPath": "home",
        "Path": "C:\\tmp\\b\\home",
        "Compress": true,
        "Excludes": [
          "asd\\bl*"
        ],
        "MaxAge": "0.0:30:00",
        "MinAge": "1.0:10:00"
      },
      {
        "Disabled": false,
        "Name": "Full-Backup",
        "UploadName": "full_$date.tar.gz",
        "UploadPath": "full",
        "Path": "C:\\tmp\\b\\home",
        "Compress": true,
        "Excludes": [
          "asd\\bl*"
        ]
      }

    ],
    "Commands": [
      {
        "Name": "Command-Backup",
        "Disabled": true,
        "UploadName": "etc_$date",
        "UploadPath": "etc",
        "Command": "powershell",
        "Arguments": "C:\\tmp\\b\\script.ps1"
      }
    ],
    "MysqlBackups": [
      {
        "Disabled": true,
        "Name": "Mysql-Backup",
        "UploadPath": "mysql",
        "UploadName": "mysql_$date.tar.gz",
        "MysqlDumpPath": "C:\\tmp\\b\\mysql\\mysqldump.exe",
        "Host": "mysql-db.dev.local",
        "Username": "root",
        "Password": "Docker_1337",
        "PasswordViaEnvironment": true,
        "Databases": [
          "alertify",
          "laravel"
        ],
        "FetchDatabases": false,
        "Excludes": [
          "information_schema",
          "performance_schema"
        ],
        "AddLocks": true,
        "Events": true,
        "FlushPrivileges": false,
        "Routines": true,
        "Triggers": true
      }
    ],
    "TarBackups": [
      {
        "Disabled": false,
        "Name": "TarBackup",
        "UploadPath": "tar",
        "UploadName": "tar_$date.tar.gz",
        "Path": "C:\\tmp\\b\\home",
        "Compress": true,
        "Excludes": [
          "/etc/etc"
        ]
      }
    ]
  },
  "Containers": [
    {
      "Path": "etc",
      "MaxSize": "1GB",
      "MaxFiles": 10
    },
    {
      "Path": "home",
      "MaxSize": "1GB",
      "MaxFiles": null
    },
    {
      "Path": "tar",
      "MaxSize": "1GB",
      "MaxFiles": 3
    },
    {
      "Path": "full",
      "MaxSize": "1GB",
      "MaxFiles": 1
    }
  ]
}